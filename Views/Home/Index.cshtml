@model List<SearchResult>

@{
    ViewBag.Title = "Search UML Design";
}

<script src="https://code.jquery.com/jquery-3.5.0.js"></script>


<button type="button" class="btn btn-primary" id="buttonUML" data-action="@Url.Action("ShowTopUML", "Search")">Top 4 UML Design</button>

<script>
    $("#buttonUML").on("click", function () {
        $.ajax({
            url: $("#buttonUML").attr("data-action"),
            data: { "searchItem": "Top 4 UML Design" },
            success: function (result) {

                window.location.href = result.url;
            }
        });
    });

</script>

